@model List<string>


<div class="text-center">
   
    @if (Model != null && Model.Count > 0)
    {
        <table class="table">
            <thead>
                <tr>
                    <th>
                        id
                    </th>
                    <th>
                        nazwa
                    </th>
                    
                </tr>
            </thead>
            <tbody>
                @{int idx =0; }
                @foreach (var agent in Model)
                {
                    <tr>
                        <td>
                            @idx++
                        </td>
                        <td>
                            @agent
                        </td>
                        
                        
                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <h2>brak dodanych danych</h2>
    }
</div>